.container {
  font-size: 14rpx;
  background: #F0F4F3;
  color: #000;
}
.answer-content{
  margin: 30rpx;
  padding: 30rpx 40rpx;
  background: #ffffff;
  border-radius: 20rpx;
  /* border: 5rpx solid #D7AB67; */
}
.muban{
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
  color: #AB0904;
  font-size: 40rpx;
  margin: 20rpx;
  text-align: center;
  border-bottom: 5rpx solid #AB0904;
}
.answer-content text{
  font-size: 32rpx;
  color: #454545;
  line-height: 44rpx;
}
.zhu{
  width: 150rpx;
  height: 70rpx;
  line-height: 70rpx;
  color: #AB0904;
  font-size: 40rpx;
  margin: 20rpx;
  text-align: center;
  border: none;
  border-bottom: 5rpx solid #AB0904;
}
.tap{
  border-bottom: 2rpx solid rgb(194, 184, 184);
  margin-bottom: 20rpx;
}
.number{
  height: 50rpx;
  width: 50rpx;
  float: left;
  background-color: #AB0904;
  color: white;
  font-size: 30rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.title{
  width: 90%;
  float: left;
}
.select{
  margin-left: 50rpx;
}
.selected text{
  color: #AB0904;
}
.answer{
  float: right;
}